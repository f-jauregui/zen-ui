{
  "name": "@reciprocity/zen-ui",
  "version": "0.0.1",
  "description": "Zen UI components library",
  "main": "dist/index.cjs.js",
  "module": "dist/index.js",
  "es2015": "dist/esm/index.mjs",
  "es2017": "dist/esm/index.mjs",
  "types": "dist/types/index.d.ts",
  "collection": "dist/collection/collection-manifest.json",
  "collection:main": "dist/collection/index.js",
  "unpkg": "dist/zen-ui/zen-ui.js",
  "files": [
    "dist/",
    "loader/"
  ],
  "scripts": {
    "start": "yarn stencil:build && (yarn stencil:watch & yarn storybook:start & yarn styles:start)",
    "build": "yarn stencil:build && yarn storybook:build && yarn styles:build",
    "test": "stencil test --spec",
    "stencil:start": "stencil build --dev --watch --serve",
    "stencil:watch": "stencil build --watch",
    "stencil:build": "stencil build --docs",
    "stencil:test:watch": "stencil test --spec --watchAll",
    "stencil:create-component": "stencil generate",
    "storybook:start": "start-storybook --docs -p 6006",
    "storybook:build": "build-storybook --docs -o public --quiet",
    "styles:build": "webpack --mode=production",
    "styles:start": "webpack --watch --mode=development"
  },
  "devDependencies": {
    "@babel/core": "~7.12.3",
    "@babel/preset-react": "~7.12.5",
    "@stencil/core": "~2.3.0",
    "@stencil/sass": "~1.4.0",
    "@storybook/addon-actions": "~6.0.28",
    "@storybook/addon-console": "~1.2.2",
    "@storybook/addon-docs": "~6.0.28",
    "@storybook/addon-essentials": "~6.0.28",
    "@storybook/addon-links": "~6.0.28",
    "@storybook/addon-notes": "~5.3.21",
    "@storybook/html": "~6.0.28",
    "@storybook/preset-scss": "~1.0.3",
    "@storybook/theming": "~6.0.28",
    "@storybook/web-components": "~6.0.28",
    "@types/jest": "~26.0.15",
    "autoprefixer": "~10.0.2",
    "babel-loader": "~8.2.1",
    "breakpoint-sass": "~2.7.1",
    "copy-webpack-plugin": "~6.3.1",
    "css-loader": "~5.0.1",
    "cssnano": "~4.1.10",
    "jest": "~26.6.3",
    "jest-cli": "~26.6.3",
    "lit-html": "~1.3.0",
    "mini-css-extract-plugin": "~1.3.1",
    "node-sass": "~5.0.0",
    "postcss": "~8.1.7",
    "postcss-loader": "~4.0.4",
    "react": "~16.14.0",
    "react-dom": "~16.14.0",
    "react-is": "~16.13.0",
    "regenerator-runtime": "^0.13.7",
    "sass-extract": "~2.1.0",
    "sass-extract-loader": "~1.1.0",
    "sass-loader": "~10.1.0",
    "style-loader": "~2.0.0",
    "webpack": "~5.4.0",
    "webpack-cli": "~4.2.0"
  },
  "license": "MIT",
  "dependencies": {
    "lodash": "~4.17.20"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/reciprocity/zen-ui.git"
  },
  "keywords": [
    "zen-ui"
  ],
  "author": "Reciprocity Inc.",
  "bugs": {
    "url": "https://github.com/reciprocity/zen-ui/issues"
  },
  "homepage": "https://github.com/reciprocity/zen-ui#readme"
}
