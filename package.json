{
  "name": "stencil-storybook-ui",
  "version": "0.0.1",
  "description": "Stencil Component with Storybook",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "es2015": "dist/esm/index.mjs",
  "es2017": "dist/esm/index.mjs",
  "types": "dist/types/index.d.ts",
  "collection": "dist/collection/collection-manifest.json",
  "collection:main": "dist/collection/index.js",
  "unpkg": "dist/stencil-storybook-starter/stencil-storybook-starter.js",
  "files": [
    "dist/",
    "loader/"
  ],
  "scripts": {
    "start": "concurrently \"npm run stencil:watch\" \"npm run storybook:start\"",
    "build": "concurrently \"npm run stencil:build\" \"npm run storybook:build\"",
    "stencil:start": "stencil build --dev --watch --serve",
    "stencil:watch": "stencil build --watch --quiet",
    "stencil:build": "stencil build --docs",
    "stencil:test": "stencil test --spec",
    "stencil:test:watch": "stencil test --spec --watchAll",
    "stencil:create-component": "stencil generate",
    "storybook:start": "start-storybook -p 6006",
    "storybook:build": "build-storybook -c .storybook -o public --quiet"
  },
  "devDependencies": {
    "@babel/core": "^7.12.3",
    "@stencil/core": "^1.17.4",
    "@storybook/addon-knobs": "^5.3.21",
    "@storybook/html": "^6.0.28",
    "@types/jest": "^26.0.15",
    "babel-loader": "^8.1.0",
    "concurrently": "^5.3.0"
  },
  "license": "MIT",
  "dependencies": {}
}
